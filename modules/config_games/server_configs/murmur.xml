<game_config>
  <game_key>mumble_linux32</game_key>
  <protocol>lgsl</protocol>
  <lgsl_query_name>murmur</lgsl_query_name>
  <game_name>Murmur [Mumble server]</game_name>
  <server_exec_name>murmur.sh</server_exec_name>
  <cli_template>-fg -v</cli_template>
  <max_user_amount>1000</max_user_amount>
  <mods>
    <mod key="murmur">
      <name>none</name>
    </mod>
  </mods>
  <replace_texts>
    <text key="ip">
	  <default>^host=.*</default>
	  <var>host=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="port">
	  <default>^port=.*</default>
	  <var>port=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="max_players">
	  <default>^users=.*</default>
	  <var>users=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="control_password">
	  <default>^icesecretwrite=.*</default>
	  <var>icesecretwrite=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
    <text key="control_password">
	  <default>^gtmurmur_icesecret=.*</default>
	  <var>gtmurmur_icesecret=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="home_name">
	  <default>^registerName=.*</default>
	  <var>registerName=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="ip">
	  <default>^gtmurmur_ip=.*</default>
	  <var>gtmurmur_ip=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="query_port">
	  <default>^gtmurmur_port=.*</default>
	  <var>gtmurmur_port=</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="incremental">
	  <default>^ice=\"tcp -h 127\.0\.0\.1 -p 6[0-9]*</default>
	  <var>ice="tcp -h 127.0.0.1 -p 6</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
	<text key="true">
	  <default>^logfile=.*</default>
	  <var>logfile=murmur.log</var>
	  <filepath>murmur.ini</filepath>
	  <options></options>
	</text>
  </replace_texts>
</game_config>
