<game_config>
  <game_key>teamspeak3_linux32</game_key>
  <protocol>teamspeak3</protocol>
  <game_name>TeamSpeak 3</game_name>
  <server_exec_name>ts3server_minimal_runscript.sh</server_exec_name>
  <cli_template> %PORT% voice_ip=%IP% query_port=10011 query_ip=%IP%</cli_template>
  <cli_params>
    <cli_param id="IP" cli_string="" />
    <cli_param id="PORT" cli_string="default_voice_port=" />
  </cli_params>
  <exe_location>teamspeak3-server_linux-x86</exe_location>
  <mods>
    <mod key="ts3">
      <name>none</name>
    </mod>
  </mods>
    <replace_texts>
	<text key="webhost_ip">
	  <default>127.0.0.1</default>
	  <var></var>
	  <filepath>teamspeak3-server_linux-x86/query_ip_whitelist.txt</filepath>
	  <options></options>
	</text>
  </replace_texts>
</game_config>

